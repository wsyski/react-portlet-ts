{
  "config": {
    "imports": {
      "react-provider": {
        "/": "*"
      }
    }
  },
  "create-jar": {
    "output-filename": "react-portlet-ts.jar",
    "features": {
      "js-extender": true,
      "web-context": "/react-portlet-ts"
    }
  },
  "dump-report": true,
  "max-parallel-files": 128,
  "verbose": true
}
